{
  "name": "todolist-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "serve:ssr:todolist-ui": "node dist/todolist-ui/server/server.mjs",
    "lint": "ng lint",
    "format:check": "prettier --check ."
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.10",
    "@angular/common": "^20.2.10",
    "@angular/compiler": "^20.2.10",
    "@angular/core": "^20.2.10",
    "@angular/forms": "^20.2.10",
    "@angular/material": "^20.2.10",
    "@angular/platform-browser": "^20.2.10",
    "@angular/platform-server": "^20.2.10",
    "@angular/router": "^20.2.10",
    "@angular/ssr": "^20.2.10",
    "@angular/animations": "^20.2.10",
    "express": "^5.1.0",
    "ngx-skeleton-loader": "^11.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.3",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.2.10",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "eslint": "^9.37.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.0"
  }
}
